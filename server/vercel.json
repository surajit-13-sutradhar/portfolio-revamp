{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 1,
    "builds": [
        {
        "src": "server.js",
        "use": "@vercel/node"
        },
        {
        "src": "src/**/*",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/" }
    ]
}